<!DOCTYPE html>
<html>
    <head>
      <title>Omrails</title>
        <%= stylesheet_link_tag "application", media:"all"%>
        <%= javascript_include_tag :defaults %>
        <%= csrf_meta_tags %>
    </head>
  <body>

    <%= render 'layouts/header' %>
    <%= javascript_include_tag :defaults %>

   <div class="container">
      <% flash.each do  |name, msg| %>
          <%= content_tag(:div, msg, class: "alert alert-#{name}")%>
      <% end %>
      <%= yield %>

      <%= render 'layouts/footer' %>
    </div>


  </body>
</html>
